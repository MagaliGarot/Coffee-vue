   <button @click=\"showColor\">Renvoie la valeur</button>\n\n    <p> Quatrième partie :</p>\n    <label for=\"username\">Nom d'utilisateur</label>\n    <input id=\"username\" type=\"text\" v-model=\"username\" />\n    <label for=\"pw\">Mot de passe</label>\n    <input id=\"pw\" type=\"password\" v-model=\"password\" />\n\n\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'HelloWorld',\n   data: () => {\n        return {\n            message : \"Menu de saison :\",\n\n            favoriteColor: 'change moi',\n\n            username: '',\n            password: '',\n          \n             simpleMenu: [\n                { label: 'Black Coffee', cost: 3 },\n                { label: 'Milk Coffee', cost: 3 },\n                { label: 'Hot Chocolate', cost: 4 },\n                { label: 'Ice Chocolate', cost: 4 },\n                { label: 'Cappuccino', cost: 3 },\n                { label: 'Gourmet coffee', cost: 10 },\n                { label: 'Water', cost: 3 },\n                { label: 'Orange juice', cost: 10 }\n            ]\n        }\n    },\n\n    methods: {\n       //changeColor relié à l'input via @keyup.enter=\"changeColor\"\n        changeColor() {\n          this.favoriteColor = document.getElementById('change-color').value\n        },\n        //showColor relié au bouton via @click=\"showColor\"\n        showColor() {\n          alert(this.favoriteColor)\n        },\n    },\n\n    computed: {\n      //1er partie\n        totalAmount() {\n          return this.costOfApples + this.costOfBananas + this.costOfCoconuts\n        },\n      //2eme partie\n        totalPanier() {\n          let total = 0\n\n          this.shoppingCart.forEach(item => {\n            total += item.cost\n          })\n          return  total\n        },\n      }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n</style>\n"]}]}